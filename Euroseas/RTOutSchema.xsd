							<xs:documentation>Sensor serial number.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProdNo" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Sensor product number.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProdName" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Sensor product name.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Adr" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>Internal AiCaP address.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ProtocolVer" type="xs:double" use="optional">
						<xs:annotation>
							<xs:documentation>Protocol version.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="VerticalPosition" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>The deployment depth (negative value) or altitude (positive value) of this particular sensor.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="GeoPosition" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation>WGS-84 coordinates in Lat/Lon decimal degrees format (DD).</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="IsVirtual" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation>True if the sensor is virtual, otherwise false.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="SystemData" type="DataNode">
		<xs:annotation>
			<xs:documentation>Represents the data from a single system node (internal device "sensor"), i.e. one or more parameters.</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
