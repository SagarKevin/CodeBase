	<xs:element name="Device">
		<xs:annotation>
			<xs:documentation>Represents the root element in any device message.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="To" type="xs:string" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The specific recipient of this message (if any). Typically used when the device sends a response to a control message.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Response" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Response to a control message.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Time" type="xs:dateTime">
					<xs:annotation>
						<xs:documentation>The UTC timestamp when the current message was compiled for transmission.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TimeReceived" type="xs:dateTime" minOccurs="0">
					<xs:annotation>
						<xs:documentation>UTC timestamp for when received by PC software such as the AADI RT Collector.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="TimeCorrection" type="xs:double" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Accumulated time correction applied to device since last power-up (decimal seconds). Only relevant for devices that support time adjustment commands.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="StatusCode" type="xs:int" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Device status code. Usually not present if everything is OK.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="SystemInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>List of static system parameters, such as location of vertical position.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="SystemInfo" minOccurs="0" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>List of static system parameters, such as location of vertical position.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="SiteInfo" minOccurs="0">
					<xs:annotation>
						<xs:documentation>OBSOLETE (replaced by the more generel SystemInformation list).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="Data" minOccurs="0" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation>Data from one or more sensors.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="con:Config" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Device configuration. Only relevant during remote configuration of the device. </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Notifications" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Notifications or alarms from the device.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="Notification" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element ref="SensorInformation" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Sensor Information (metadata).</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element ref="layout:Layout" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Device specific installation settings. Only relevant during remote configuration of the device. </xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="ID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Globally unique device ID, usually built from the device product number and serial number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SessionID" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Session ID. updated when the device config changes.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="Descr" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>User-defined device description.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="SerialNo" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Device serial number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProdNo" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Device product number.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProdName" type="xs:string" use="optional">
				<xs:annotation>
					<xs:documentation>Device product name.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="DeviceType" type="DeviceType" use="optional">
				<xs:annotation>
					<xs:documentation>Device type (Instrument or Sensor).</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="ProtocolVer" type="xs:double" use="optional">
				<xs:annotation>
					<xs:documentation>Version number of the AADI Real-Time Output Protocol.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="SensorDependency">
